{
  "$schema": "http://json.schemastore.org/template",
  "author": "Olga Nelioubov",
  "classifications": [
    "Api",
    "Code",
    "Reference"
  ],
  "identity": "Dotnet.Template.Demo",
  "name": "Demo Template",
  "shortName": "demo-template",
  "tags": {
    "language": "C#",
    "type": "item"
  },
  "symbols": {
    "serviceName": {
      "type": "parameter",
      "datatype": "string",
      "description": "This is what the solution will be called, along with the corresponding API and Tests projects",
      "defaultValue": "generic",
      "FileRename": "DotnetTemplateDemo",
      "replaces": "DotnetTemplateDemo",
      "isRequired": true
    },
    "lowerCaseServiceName": {
      "type": "generated",
      "generator": "casing",
      "parameters": {
        "source": "serviceName",
        "toLower": true
      },
      "replaces": "dotnettemplatedemo"
    }
  },
  "sources":[{
    "modifiers": [
      {
        "exclude": [
          "**/[Bb]in/**",
          "**/[Oo]bj/**",
          ".template.config/**/*",
          "**/*.filelist",
          "**/*.user",
          "**/*.lock.json",
          "**/.vs/**",
          "**/.vscode/**",
          "**/.git/**",
          "packageSettings.csproj",
          "**/.tmp/**",
          ".idea/**",
          "**/artifacts/**",
          "**/.terraform/**"
        ]
      }
    ]
  }]
}